{
  "name": "cloud_storage_server",
  "version": "0.0.1",
  "description": "Cloud Storage Server",
  "main": "./server/server.js",
  "scripts": {
    "test": "node ./tests/test",
    "connect_db": "psql -U postgres -d cloud_storage",
    "prepare_db": "psql -U postgres -af ./src/db/install.sql && psql -U admin -d cloud_storage -af ./src/db/structure.sql",
    "prepare_static": "cd ./src/react_cloud_client_web && npm i",
    "first_step": "npm i && npm run prepare_static && npm run prepare_db && npm run build",
    "add_user": "node addUser",
    "first_start": "npm run first_step && npm run add_user && npm start",
    "compile_server": "npm run tsc",
    "lite": "npx tsc && node ./build.js && cd ./target && node ./server.js",
    "build": "npm run tsc && npm run build --prefix ./src/react_cloud_client_web && node ./build.js",
    "start": "npm run build && cd ./target && node ./server.js",
    "tsc": "tsc"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/AliusDieMorietur/Cloud_Storage_Server.git"
  },
  "author": "AliusDieMorietur",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/AliusDieMorietur/Cloud_Storage_Server/issues"
  },
  "homepage": "https://github.com/AliusDieMorietur/Cloud_Storage_Serverr#readme",
  "devDependencies": {
    "@types/node": "^14.14.25",
    "@types/ws": "^7.4.0",
    "typedoc": "^0.20.25",
    "typescript": "^4.1.3"
  },
  "dependencies": {
    "pg": "^8.5.0",
    "ws": "^7.4.0"
  },
  "keywords": []
}
